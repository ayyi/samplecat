#include "stdio.h"

typedef enum {
	PANEL_TYPE_XX,
	PANEL_TYPE_MAX
} PanelType;

@REGISTER1

void register_panels ()
{
	dock_registry_init();

	@REGISTER2

	for (int p=0;p<G_N_ELEMENTS(panels);p++) {
		Panel* panel = &panels[p];
		if (!panel->gtype)
			register_gtk_fn(panel->name, panel->new);
	}
}

